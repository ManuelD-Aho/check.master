{
    "name": "miage-gi/plateforme-stages",
    "description": "Plateforme de Gestion des Stages et Soutenances de Master MIAGE-GI",
    "type": "project",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.3",
        "brick/math": "^0.12",
        "defuse/php-encryption": "^2.4",
        "doctrine/dbal": "^4.0",
        "doctrine/orm": "^3.0",
        "egulias/email-validator": "^4.0",
        "ezyang/htmlpurifier": "^4.17",
        "filp/whoops": "^2.15",
        "laminas/laminas-httphandlerrunner": "^2.10",
        "lcobucci/jwt": "^5.0",
        "league/csv": "^9.0",
        "monolog/monolog": "^3.5",
        "nesbot/carbon": "^3.0",
        "nikic/fast-route": "^1.3",
        "nyholm/psr7": "^1.8",
        "nyholm/psr7-server": "^1.1",
        "pagerfanta/pagerfanta": "^4.0",
        "php-di/php-di": "^7.0",
        "phpmailer/phpmailer": "^6.9",
        "psr/http-server-middleware": "^1.0",
        "respect/validation": "^2.3",
        "spomky-labs/otphp": "^11.0",
        "symfony/event-dispatcher": "^7.0",
        "symfony/password-hasher": "^7.0",
        "symfony/security-csrf": "^7.0",
        "symfony/string": "^7.0",
        "symfony/workflow": "^7.0",
        "tecnickcom/tcpdf": "^6.6",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit"
    }
}
